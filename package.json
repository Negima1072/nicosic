{
  "name": "nicosic",
  "version": "0.0.1",
  "description": "Music Player Application for niconico",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "dev": "rimraf dist && run-p dev:*",
    "dev:tsc": "tsc -w -p tsconfig.node.json",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./dist/index.html ./dist/main.js && cross-env NODE_ENV=\"development\" electron .",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "start": "cross-env NODE_ENV=\"production\" electron .",
    "package:win": "npm run build && electron-builder --win --x64",
    "package:mac": "npm run build && electron-builder --mac --x64"
  },
  "keywords": [],
  "author": "Negima1072",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "electron-store": "^8.1.0",
    "express": "^4.18.3",
    "http-cookie-agent": "^6.0.1",
    "node-fetch": "^2.7.0",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.20",
    "@types/tough-cookie": "^4.0.5",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "electron": "^29.1.0",
    "electron-builder": "^24.13.3",
    "electron-reload": "2.0.0-alpha.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "fit-curve": "^0.2.0",
    "hls.js": "^1.5.7",
    "html-webpack-plugin": "^5.6.0",
    "jotai": "^2.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react": "^18.2.0",
    "react-contexify": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.2",
    "react-spinners": "^0.13.8",
    "rimraf": "^5.0.5",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
